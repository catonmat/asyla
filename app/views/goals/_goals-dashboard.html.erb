<% @goals.each do |goal| %>
  <div class="card-goal" id="<%= goal.category.name %>">
    <p id="goal-name"><%= goal.name %></p>
    <% @dashboard.each do |goal_object| %>
      <% if goal.name == goal_object[:goal_name] %>
        <% goal_object[:progress] %>
          <div>
            <p><span id="large-label"><%= goal_object[:completed_tasks] %></span> of <span id="large-label"><%= goal_object[:total_tasks] %></span> tasks complete</p>
          </div>

      <% end %>
    <% end %>
  </div>
<% end %>



